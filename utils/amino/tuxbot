commands = {"/hello": ["Devuelve un saludo.", "Say"], "/hi": ["Devuelve un saludo.", "Say"], "/help": ["Muestra este mensaje.", "Say"], "/version": ["Muestra la versión del bot.", "Say"], "/di": ["Repite lo que se le pide.", "Say"], "/meme": ["Muestra una plantilla, si no se especifican argumentos muestra un meme aleatorio.", "Entertainment"], "/public_chats": ["Muestra los chats publicos.", "Info"], "/info": ["Revela información de un usuario.", "Info"], "/goto": ["Envía el bot a otro chat.", "Bot"], "/mv": ["Envía el bot a otra comunidad.", "Bot"], "/stand": ["Previene que el bot vuelva al chat de bots.", "Chat"], "/guard": ["Repite los mensajes borrados durante N cantidad de tiempo.", "Chat"], "/id": ["Revela la Id del chat y de la comunidad.", "Chat"], "/eval": ["Evalúa una expresión matemática.", "Utils"], "/teach": ["Enseña un comando y su respuesta al bot.", "Entertainment"], "/trad": ["Traduce una oración a X idioma.", "Utils"], "/langdet": ["Detecta el idioma de una oración.", "Utils"], "/def": ["Busca la definición de un término.", "Utils"], "/stop": ["Termina el proceso.", "Bot"]}
